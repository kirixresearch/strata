msgid "GREATEST(value1 [, value2, ...])"
msgstr "GREATEST(valor1 [, valor2, ...])"

#: appmain/exprbuilder.cpp:80
msgid "Returns the largest value from the input parameters"
msgstr "Devuelve el valor más grande de los parámetros de entrada"

#: appmain/exprbuilder.cpp:81
msgid "HOUR(date)"
msgstr "HOUR(fecha)"

#: appmain/exprbuilder.cpp:81
msgid "Returns the hour corresponding to a date"
msgstr "Devuelve la hora correspondiente a una fecha"

